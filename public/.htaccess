RewriteRule ^special/s10$ /#/s/s10_4aa3 [R=301,L]
RewriteRule ^special/s11$ /#/s/s11_e846 [R=301,L]
RewriteRule ^special/s12$ /#/s/s12_9c55 [R=301,L]
RewriteRule ^special/s13$ /#/s/s13_68a6 [R=301,L]
