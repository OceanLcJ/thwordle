# Apache重写规则 - 支持SEO友好URL
RewriteEngine On

# 训练模式
RewriteRule ^training$ /#/training [R=301,L]

# 特殊关卡路由
RewriteRule ^special/special1$ /#/s/special1 [R=301,L]
